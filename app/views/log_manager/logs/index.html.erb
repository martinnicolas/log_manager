<div>
  <table>
    <thead>
      <tr>
        <th>LogManager v <%= LogManager::VERSION %> | <%= t :environment %>: <%= @environment %> | <%= t :file_size %>: <%= @file_size %></th>
      </tr>
    </thead>
    <tbody>
      <% @file_data.each do |line| %>
        <tr>
          <td><%= line %></td>
        </tr>
      <% end %>
    </tbody>
  </table>  
</div>
